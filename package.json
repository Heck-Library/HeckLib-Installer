{
  "name": "hecklib",
  "displayName": "HeckLib",
  "description": "",
  "publisher": "Heck-Library",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "./src/resources/heckLib_logo.png",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "HeckLib",
          "title": "HeckLib",
          "icon": "./src/resources/heckLib_vsc.png"
        }
      ]
    },
    "views": {
      "HeckLib": [
        {
          "id": "HeckLib",
          "name": "Downloader / Updater"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "HeckLib",
        "contents": "[Download Hecklib](command:test.Download-HeckLib)",
        "when": "myExtension.file == false"
      },
      {
        "view": "HeckLib",
        "contents": "Here you can update this shit \n[Update Hecklib](command:test.Update-HeckLib)",
        "when": "myExtension.file == true"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.1.1",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "path": "^0.12.7",
    "typescript": "^4.9.5"
  }
}
